{
  "parser"  : "babel-eslint",
  "extends" : ["airbnb"],
  "env"     : {
    "browser" : true
  },
  "globals" : {
    "__DEV__": false
  },
  "rules": {
    "react/no-did-mount-set-state": 0,
    "react/forbid-prop-types": 0,
    "react/require-default-props": 0
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./webpack.config.js"
      }
    }
  }
}
